<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black_de">#DE000000</color>
    <color name="card_background">#ffffff</color>
    <color name="card_shadow">#10000000</color>
    <color name="lite_blue">#ff359ff2</color>
    <color name="window_background">#00000000</color>
    <declare-styleable name="RecyclerView"><attr format="string" name="layoutManager"/><attr name="android:orientation"/><attr name="android:descendantFocusability"/><attr format="integer" name="spanCount"/><attr format="boolean" name="reverseLayout"/><attr format="boolean" name="stackFromEnd"/></declare-styleable>
    <declare-styleable name="SwitchButton"><attr format="reference|color" name="frameDrawable"/><attr format="reference|color" name="stateDrawable"/><attr format="reference|color" name="stateMaskDrawable"/><attr format="reference|color" name="sliderDrawable"/><attr format="dimension" name="withTextInterval"/></declare-styleable>
    <dimen name="action_button_height">32dp</dimen>
    <dimen name="action_button_min_width">56dp</dimen>
    <dimen name="action_button_padding_horizontal">4dp</dimen>
    <dimen name="action_button_text_size">14sp</dimen>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="item_touch_helper_max_drag_scroll_per_frame">20dp</dimen>
    <dimen name="item_touch_helper_swipe_escape_max_velocity">800dp</dimen>
    <dimen name="item_touch_helper_swipe_escape_velocity">120dp</dimen>
    <item name="item_touch_helper_previous_elevation" type="id"/>
    <string name="action_settings">Settings</string>
    <string name="app_name">矩阵拦截</string>
    <string name="hello_world">Hello world!</string>
    <style name="ActionButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/action_button_height</item>
        <item name="android:minWidth">@dimen/action_button_min_width</item>
        <item name="android:paddingLeft">@dimen/action_button_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/action_button_padding_horizontal</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:textSize">@dimen/action_button_text_size</item>
    </style>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="buttonItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:layout_marginLeft">-20dp</item>
        <item name="android:layout_marginRight">-20dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingRight">30dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:minHeight">52dp</item>
        <item name="android:drawablePadding">16dp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="item">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:layout_marginLeft">-20dp</item>
        <item name="android:layout_marginRight">-20dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingRight">30dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:minHeight">52dp</item>
        <item name="android:drawablePadding">16dp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
</resources>